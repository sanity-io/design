{
  "name": "@sanity/tsdoc-to-portable-text",
  "version": "0.3.0",
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./lib/tsdoc-to-portable-text.js",
  "module": "./lib/tsdoc-to-portable-text.module.js",
  "types": "./lib/dts/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./lib/tsdoc-to-portable-text.js",
      "module": "./lib/tsdoc-to-portable-text.module.js",
      "default": "./lib/tsdoc-to-portable-text.modern.js"
    },
    "./cli": {
      "source": "./src/cli/main.ts",
      "require": "./lib/tsdoc-to-portable-text-cli.js"
    }
  },
  "bin": {
    "tsdoc-to-portable-text": "./bin/tsdoc-to-portable-text"
  },
  "files": [
    "lib",
    "src"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "build": "../../../bin/pkg-utils bundle --target node --tsconfig tsconfig.lib.json --validate",
    "clean": "rimraf build lib",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "prebuild": "yarn clean",
    "test": "jest",
    "type-check": "tsc --build",
    "watch": "../../../bin/pkg-utils bundle --target node --tsconfig tsconfig.lib.json --watch"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.24.2",
    "@microsoft/api-extractor-model": "^7.17.3",
    "@microsoft/tsdoc": "^0.14.1",
    "@microsoft/tsdoc-config": "^0.16.1",
    "@types/cpx": "^1.5.2",
    "chalk": "^4.1.2",
    "jsonc-parser": "^3.0.0",
    "mkdirp": "^1.0.4",
    "pkg-up": "^3.1.0",
    "slugify": "^1.6.5",
    "tmp": "^0.2.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.2",
    "@types/tmp": "^0.2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
