{
  "name": "@sanity/logos",
  "version": "1.1.15",
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pkg-utils build --extract --tsconfig tsconfig.dist.json",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "prebuild": "yarn clean",
    "test": "jest",
    "type-check": "tsc --build",
    "watch": "run-p watch:*",
    "watch:cjs": "tsc --preserveWatchOutput -w --module CommonJS --outDir dist/cjs --project tsconfig.dist.json --rootDir src --target ES5",
    "watch:es": "tsc --preserveWatchOutput -w --module ES6 --outDir dist/esm --project tsconfig.dist.json --rootDir src --target ES6"
  },
  "devDependencies": {
    "@sanity/color": "^2.1.14",
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "@sanity/color": "^2.0",
    "react": "^16.9 || ^17 || ^18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/design.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/design/issues"
  },
  "keywords": [
    "sanity",
    "ui",
    "logos"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "publishConfig": {
    "access": "public"
  }
}
