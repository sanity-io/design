{
  "name": "@sanity/color",
  "version": "2.1.9",
  "description": "The Sanity color palette.",
  "sideEffects": false,
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./lib/sanity-color.js",
      "default": "./lib/sanity-color.modern.js"
    }
  },
  "main": "./lib/sanity-color.js",
  "module": "./lib/sanity-color.module.js",
  "types": "./lib/dts/src/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "../../../bin/pkg-utils bundle --tsconfig tsconfig.lib.json --validate",
    "clean": "rimraf lib",
    "generate": "node -r esbuild-register scripts/generate.ts",
    "lint": "eslint . --ext .js,.ts --quiet",
    "prebuild": "yarn clean && yarn generate",
    "test": "jest",
    "type-check": "tsc --build",
    "watch": "../../../bin/pkg-utils bundle --tsconfig tsconfig.lib.json --watch"
  },
  "devDependencies": {
    "polished": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/design.git"
  },
  "keywords": [
    "sanity",
    "color",
    "palette"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/design/issues"
  },
  "homepage": "https://www.sanity.io/",
  "publishConfig": {
    "access": "public"
  }
}
