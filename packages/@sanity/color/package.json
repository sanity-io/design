{
  "name": "@sanity/color",
  "version": "2.1.14",
  "description": "The Sanity color palette.",
  "sideEffects": false,
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pkg-utils build --extract --tsconfig tsconfig.dist.json",
    "clean": "rimraf dist",
    "generate": "node -r esbuild-register scripts/generate.ts",
    "lint": "eslint . --ext .js,.ts --quiet",
    "prebuild": "yarn clean && yarn generate",
    "test": "jest",
    "type-check": "tsc --build",
    "watch": "run-p watch:*",
    "watch:cjs": "tsc --preserveWatchOutput -w --module CommonJS --outDir dist/cjs --project tsconfig.dist.json --rootDir src --target ES5",
    "watch:es": "tsc --preserveWatchOutput -w --module ES6 --outDir dist/esm --project tsconfig.dist.json --rootDir src --target ES6"
  },
  "devDependencies": {
    "polished": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/design.git"
  },
  "keywords": [
    "sanity",
    "color",
    "palette"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/design/issues"
  },
  "homepage": "https://www.sanity.io/",
  "publishConfig": {
    "access": "public"
  }
}
